{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Equation Calculator",
  "version": "0.2.0",
  "identifier": "com.tauri-app.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Equation Calculator",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../src/assets/favicon.ico"
    ]
  }
}
